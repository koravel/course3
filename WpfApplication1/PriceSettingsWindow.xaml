<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit" x:Class="WpfApplication1.PriceSettingsWindow"
        Title="PriceSettingsWindow" Height="300" Width="300">
    <Grid>

        <xctk:IntegerUpDown x:Name="upDownPriceProc" FormatString="" Value="0" Increment="1" Maximum="100" Minimum="0" HorizontalAlignment="Left" Width="120" Height="20" VerticalAlignment="Top" Margin="34,43,0,0" IsEnabled="False"/>
        <TextBlock HorizontalAlignment="Left" Margin="159,43,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="20" IsEnabled="False"><Run Language="ru-ru" Text="%" IsEnabled="False"/></TextBlock>
        <Button x:Name="buttonPriceSettingsSave" Content="Сохранить" HorizontalAlignment="Left" Margin="10,238,0,0" VerticalAlignment="Top" Width="75" Click="buttonPriceSettingsSave_Click"/>
        <Button x:Name="buttonPriceSettingsBack" Content="Назад" HorizontalAlignment="Left" Margin="207,238,0,0" VerticalAlignment="Top" Width="75" Click="buttonPriceSettingsBack_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="34,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top"><Run Language="ru-ru" Text="Розничная цена"/></TextBlock>

    </Grid>
</Window>

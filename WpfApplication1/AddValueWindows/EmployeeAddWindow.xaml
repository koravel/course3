<Window x:Class="WpfApplication1.EmployeeAddWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="EmployeeAddWindow" Height="314" Width="190">
    <Grid>
        <TextBox x:Name="textBoxName" HorizontalAlignment="Left" Height="23" Margin="23,39,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
